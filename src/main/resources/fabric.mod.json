{
  "schemaVersion": 1,
  "id": "daydisplayermod",
  "version": "${version}",
  "name": "DayDisplayer",
  "description": "This mod displays what minecraft day you are on",
  "authors": [
    "Me!"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "MIT",
  "icon": "assets/daydisplayermod/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.minecrafteur.daydisplayermod.DayDisplayer"
    ],
    "client": [
      "com.minecrafteur.daydisplayermod.DayDisplayerClient"
    ],
    "server": [
      "com.minecrafteur.daydisplayermod.DayDisplayerServer"
    ],
    "fabric-datagen": [
      "com.minecrafteur.daydisplayermod.DayDisplayerDataGenerator"
    ]
  },
  "mixins": [
    "daydisplayermod.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.25",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "fabric-api": "*",
